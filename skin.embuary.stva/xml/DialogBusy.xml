<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- busydialog -->
	<include>PropertyIncludes</include>
	<controls>
		<control type="group">
			<visible>!Window.IsVisible(home)</visible>
			<include content="DiffuseMask"/>
			<include>BusySpinner</include>
		</control>
		<control type="group">
			<visible>Window.IsVisible(home) + !Skin.HasSetting(autotrailer)</visible>
			<include content="DiffuseMask"/>
			<include>BusySpinner</include>
		</control>
	</controls>
</window>