					<visible>Library.HasContent(tvshows</visible>
				</control>
				<control type="image" id="2">
					<width>100%</width>
					<height>61</height>
					<texture border="65,0,65,0" colordiffuse="context_color">dialogs/context/bottom.png</texture>
				</control>
			</control>
		</control>
