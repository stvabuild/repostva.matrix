					<include>SettingsButton</include>
				</control>
				<control type="image" id="11">
					<description>separator image</description>
					<visible>false</visible>
				</control>
				<include content="SkinSettingsHeadlineLabel">
					<param name="id" value="14"/>
				</include>
			</control>
