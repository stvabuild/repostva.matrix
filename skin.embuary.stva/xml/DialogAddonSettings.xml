					<control type="grouplist" id="1000">
						<left>0</left>
						<top>50</top>
						<usecontrolcoords>true</usecontrolcoords>
						<onleft>noop</onleft>
						<onright>5</onright>
						<onup>3</onup>
						<ondown>noop</ondown>
						<onback>1100</onback>
						<control type="button" id="1001">
							<label>$LOCALIZE[732]</label>
							<onclick>SendClick(28)</onclick>
							<include>SettingsButtonSidePanel</include>
