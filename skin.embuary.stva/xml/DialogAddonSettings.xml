				<left>450</left>
				<right>0</right>
				<height>1</height>
				<texture colordiffuse="grid_border">common/white.jpg</texture>
				<aspectratio>stretch</aspectratio>
			</control>
			<control type="image">
				<bottom>112</bottom>
				<left>450</left>
				<right>0</right>
				<height>1</height>
				<texture colordiffuse="grid_border">common/white.jpg</texture>
				<aspectratio>stretch</aspectratio>
