						<control type="button" id="1003">
							<label>$LOCALIZE[31135]</label>
							<onclick>SendClick(30)</onclick>
							<include>SettingsButtonSidePanel</include>
						</control>
						<control type="button" id="1002">
							<label>$LOCALIZE[222]</label>
							<onclick>SendClick(29)</onclick>
							<include>SettingsButtonSidePanel</include>
						</control>
					</control>
				</control>
			</control>
			<!-- right -->
			<control type="group">
				<left>510</left>
				<right>50</right>
				<top>113</top>
				<bottom>113</bottom>
