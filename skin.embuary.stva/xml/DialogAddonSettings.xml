				<control type="grouplist" id="5">
					<height>100%</height>
					<onup>6666</onup>
					<onleft>3</onleft>
					<onright>noop</onright>
					<ondown>1001</ondown>
					<pagecontrol>60</pagecontrol>
					<scrolltime>200</scrolltime>
