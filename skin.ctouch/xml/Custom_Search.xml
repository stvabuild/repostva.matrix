<?xml version="1.0" encoding="utf-8"?>
<window type="dialog" id="1132">
	<defaultcontrol>1999</defaultcontrol>
	<coordinates>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<control type="image">
			<include>fullscreenimage</include>
			<texture>background-mask-black.png</texture>
			<aspectratio>scale</aspectratio>
		</control>
		<control type="button">
			<!-- Touch mouse close button -->
			<right>15</right>
			<bottom>15</bottom>
			<width>180</width>
			<height>75</height>
			<texturefocus colordiffuse="$VAR[AccentColor]">backtransparent.png</texturefocus>
			<texturenofocus colordiffuse="midgrey">backtransparent.png</texturenofocus>
			<onclick>Dialog.Close(1132)</onclick>
			<align>center</align>
			<aligny>center</aligny>
			<label>Cerrar</label>
			<font>ctouch19</font>
			<visible>Skin.HasSetting(menulittle)</visible>
		</control>
		<control type="panel" id="1999">
			<centerleft>50%</centerleft>
			<centertop>51%</centertop>
			<width>520</width>
			<height>500</height>
			<onleft>Close</onleft>
			<onright>Close</onright>
			<aligny>top</aligny>
			<itemlayout width="520" height="75">
				<control type="image">
					<centerleft>5%</centerleft>
					<top>20</top>
					<width>40</width>
					<height>40</height>
					<texture diffuse="diffuse/square.png">$INFO[ListItem.Art(thumb)]</texture>
					<aspectratio>scale</aspectratio>
				</control>	
				<control type="label">
					<centerleft>50%</centerleft>
					<align>center</align>
					<top>20</top>
					<width>460</width>
					<height>36</height>
					<font>ctouch22</font>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</itemlayout>
			<focusedlayout width="520" height="75">
				<control type="image">
					<left>0</left>
					<top>0</top>
					<width>520</width>
					<height>75</height>
					<texture border="9" colordiffuse="$VAR[AccentColor]">button-default-white.png</texture>
				</control>
				<control type="image">
					<centerleft>5%</centerleft>
					<top>20</top>
					<width>40</width>
					<height>40</height>
					<texture diffuse="diffuse/square.png">$INFO[ListItem.Art(thumb)]</texture>
					<aspectratio>scale</aspectratio>
				</control>	
				<control type="label">
					<centerleft>50%</centerleft>
					<align>center</align>
					<top>20</top>
					<width>460</width>
					<height>36</height>
					<font>ctouch22</font>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</focusedlayout>
			<content>
				<item>
					<thumb>icons/palantir.png</thumb>
					<label>Buscador Palantir</label>
					<onclick>Dialog.Close(all)</onclick>
					<onclick condition="System.HasAddon(plugin.video.palantir3) + Skin.HasSetting(bpalantir)">ActivateWindow(10025,&quot;plugin://plugin.video.palantir3/?&quot;)</onclick>
					<onclick condition="!System.HasAddon(plugin.video.palantir3) + Skin.HasSetting(bpalantir)">InstallAddon(plugin.video.palantir3)</onclick>
					<visible>!Skin.HasSetting(buscador-palantir)</visible>
				</item>
				<item>
					<thumb>icons/alfa.png</thumb>
					<label>Buscador Alfa</label>
					<onclick>Dialog.Close(all)</onclick>
					<onclick condition="System.HasAddon(plugin.video.alfa)">ActivateWindow(10025,&quot;plugin://plugin.video.alfa/?&quot;,return)</onclick>
					<onclick condition="!System.HasAddon(plugin.video.alfa)">InstallAddon(plugin.video.alfa)</onclick>
					<visible>!Skin.HasSetting(buscador-alfa)</visible>
				</item>
				<item>
					<thumb>$INFO[Skin.String(CustomSearch.1.Icon)]</thumb>
					<label>$INFO[Skin.String(CustomSearch.1.Label)]</label>
					<onclick>Dialog.Close(all)</onclick>
					<onclick>$INFO[Skin.String(customsearch.1.path)]</onclick>
					<visible>!String.IsEmpty(Skin.String(CustomSearch.1.Path))</visible>
				</item>
				<item>
					<thumb>$INFO[Skin.String(CustomSearch.2.Icon)]</thumb>
					<label>$INFO[Skin.String(CustomSearch.2.Label)]</label>
					<onclick>Dialog.Close(all)</onclick>
					<onclick>$INFO[Skin.String(customsearch.2.path)]</onclick>
					<visible>!String.IsEmpty(Skin.String(CustomSearch.2.Path))</visible>
				</item>
				<item>
					<thumb>$INFO[Skin.String(CustomSearch.3.Icon)]</thumb>
					<label>$INFO[Skin.String(CustomSearch.3.Label)]</label>
					<onclick>Dialog.Close(all)</onclick>
					<onclick>$INFO[Skin.String(CustomSearch.3.Path)]</onclick>
					<visible>!String.IsEmpty(Skin.String(CustomSearch.3.Path))</visible>
				</item>
			</content>
		</control>
	</controls>
</window>