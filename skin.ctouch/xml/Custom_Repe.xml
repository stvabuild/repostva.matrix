<?xml version="1.0" encoding="utf-8"?>
<window type="dialog" id="1134">
	<defaultcontrol>1999</defaultcontrol>
	<coordinates>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<control type="image">
			<animation effect="fade" delay="5000" start="0" end="100" time="1000" condition="true">Conditional</animation>
			<include>fullscreenimage</include>
			<texture colordiffuse="mask-background">background-mask.png</texture>
			<aspectratio>scale</aspectratio>
		</control>
		<control type="image">
			<!-- Dialog background image -->
			<centerleft>50%</centerleft>
			<centertop>50%</centertop>
			<width>570</width>
			<height>360</height>
			<texture border="14">button-menu-background.png</texture>
		</control>
		<control type="button">
			<!-- Touch mouse close button -->
			<centerleft>50%</centerleft>
			<top>630</top>
			<width>64</width>
			<height>64</height>
			<texturefocus>touch-close.png</texturefocus>
			<texturenofocus>touch-close.png</texturenofocus>
			<onclick>Close</onclick>
		</control>
		<control type="panel" id="1999">
			<centerleft>50%</centerleft>
			<centertop>51%</centertop>
			<width>540</width>
			<height>350</height>
			<onleft>Close</onleft>
			<onright>Close</onright>
			<aligny>top</aligny>
			<itemlayout width="540" height="75">
				<control type="label">
					<centerleft>50%</centerleft>
					<align>center</align>
					<top>20</top>
					<width>540</width>
					<height>36</height>
					<font>ctouch22</font>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</itemlayout>
			<focusedlayout width="540" height="75">
				<control type="image">
					<left>0</left>
					<top>0</top>
					<width>540</width>
					<height>75</height>
					<texture border="9" colordiffuse="$VAR[AccentColor]">button-default-white.png</texture>
				</control>
				<control type="label">
					<centerleft>50%</centerleft>
					<align>center</align>
					<top>20</top>
					<width>540</width>
					<height>36</height>
					<font>ctouch22</font>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</focusedlayout>
			<content>
				<item>
					<label>MotoGP</label>
					<onclick>Dialog.Close(all)</onclick>
					<onclick condition="System.HasAddon(plugin.video.alfa)">ActivateWindow(10025,&quot;plugin://plugin.video.alfa/?&quot;,return)</onclick>
					<onclick condition="!System.HasAddon(plugin.video.alfa)">InstallAddon(plugin.video.alfa)</onclick>
				</item>
				<item>
					<label>Formula 1</label>
					<onclick>Dialog.Close(all)</onclick>
					<onclick condition="System.HasAddon(plugin.video.alfa)">ActivateWindow(10025,&quot;plugin://plugin.video.alfa/?%3d&quot;,return)</onclick>
					<onclick condition="!System.HasAddon(plugin.video.alfa)">InstallAddon(plugin.video.alfa)</onclick>
					</item>
				<item>
					<label>Repeticiones Cristal Azul</label>
					<onclick>Dialog.Close(all)</onclick>
					<onclick condition="System.HasAddon(plugin.video.cristalazul)">ActivateWindow(Videos,&quot;plugin://plugin.video.cristalazul/?fanart=https%3a%2f%2fi.imgur.com%2f932XIJt.jpg&amp;mode=1&amp;name=%5bCOLOR%20gold%5d%2a%20%5bCOLOR%20lightgreen%5dRepeticiones%20%5bCOLOR%20white%5dDeportivas%5b%2fCOLOR%5d%20&amp;url=https%3a%2f%2fpastebin.com%2fraw%2fdNkEPZ4J&quot;,return)</onclick>
					<onclick condition="!System.HasAddon(plugin.video.cristalazul)">InstallAddon(plugin.video.cristalazul)</onclick>
				</item>
			</content>
		</control>
	</controls>
</window>