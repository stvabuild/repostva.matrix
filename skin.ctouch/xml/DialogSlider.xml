<?xml version="1.0" encoding="utf-8"?>
<window>
	<defaultcontrol>11</defaultcontrol>
	<coordinates>
		<left>424</left>
		<top>30</top>
	</coordinates>
	<controls>
		<control type="group">
			<visible>![Window.IsVisible(notification) | Window.IsVisible(volumebar)]</visible>
			<control type="label" id="10">
				<!-- Dialog heading -->
				<left>33</left>
				<top>27</top>
				<width>477</width>
				<height>30</height>
				<aligny>center</aligny>
				<font>ctouch19</font>
			</control>
			<control type="button">
				<!-- Touch Close button -->
				<left>515</left>
				<top>-5</top>
				<width>64</width>
				<height>64</height>
				<texturefocus>touch-close.png</texturefocus>
				<texturenofocus>touch-close.png</texturenofocus>
				<onclick>Dialog.Close(sliderdialog)</onclick>
				<visible>Skin.HasSetting(menulittle)</visible>
			</control>
			<control type="label" id="12">
				<!-- Slider value -->
				<left>555</left>
				<top>27</top>
				<width>477</width>
				<height>30</height>
				<align>right</align>
				<aligny>center</aligny>
				<font>ctouch19</font>
			</control>
			<control type="image">
				<!-- Seek slider background -->
				<left>27</left>
				<top>76</top>
				<width>1008</width>
				<height>14</height>
				<texture border="12,0,12,0">progress-bar-focus.png</texture>
				<colordiffuse>ProgressBarColor</colordiffuse>
			</control>
			<control type="slider" id="11">
				<left>27</left>
				<top>76</top>
				<width>1008</width>
				<height>14</height>
				<texturesliderbar border="10" colordiffuse="$VAR[AccentColor]">progress-bar-focus.png</texturesliderbar>
				<textureslidernib>progress-bar-focus.png</textureslidernib>
				<textureslidernibfocus>progress-bar-focus.png</textureslidernibfocus>
			</control>
		</control>
	</controls>
</window>