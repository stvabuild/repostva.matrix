			<texturenofocus colordiffuse="midgrey">backtransparent.png</texturenofocus>
			<onclick>Dialog.Close(1139)</onclick>
			<align>center</align>
			<aligny>center</aligny>
			<label>Cerrar</label>
			<font>ctouch19</font>
			<visible>Skin.HasSetting(menulittle)</visible>
		</control>
		<control type="group">
			<centerleft>50%</centerleft>
			<width>750</width>
			<top>100</top>
			<height>1080</height>
			<control type="group">
				<onright>Dialog.close</onright>
				<onleft>Dialog.close</onleft>
				<onup>Dialog.close</onup>
				<ondown>Dialog.close</ondown>
				<control type="image">
					<width>750</width>
					<height>422</height>
					<texture diffuse="diffuse/landscape.png">special://skin/extras/images/extras/waces.jpg</texture>
